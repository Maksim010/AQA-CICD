{
  "chrome": {
    "default": "125.0",
    "versions": {
      "125.0": {
        "image": "selenoid/chrome:125.0",
        "port": "4444",
        "path": "/",
        "env": [
          "TZ=UTC",
          "LANG=ru_RU.UTF-8"
        ],
        "volumes": [
          "/tmp:/tmp",
          "/var/run/docker.sock:/var/run/docker.sock"
        ],
        "tmpfs": {
          "/tmp": "size=512m"
        }
      },
      "beta": {
        "image": "selenoid/chrome:beta",
        "port": "4444"
      }
    }
  },
  "firefox": {
    "default": "120.0",
    "versions": {
      "120.0": {
        "image": "selenoid/firefox:120.0",
        "port": "4444"
      }
    }
  },
  "edge": {
    "default": "latest",
    "versions": {
      "latest": {
        "image": "selenoid/edge:latest",
        "port": "4444"
      }
    }
  },
  "opera": {
    "default": "99.0",
    "versions": {
      "99.0": {
        "image": "selenoid/opera:99.0",
        "port": "4444"
      }
    }
  }
}